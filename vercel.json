{
    "version": 2,
    "builds": [   
      { "src": "*.html", "use": "@vercel/static" },   
      { "src": "api/*.py", "use": "@vercel/python" } 
    ],
    "routes": [
      { "src": "/", "dest": "/index.html" },          
      { "src": "/(.*)", "dest": "/api/api.py" },
      { "src": "/api/(.*)", "dest": "/api/web.py" },  
      { "src": "/admin", "dest": "/index.html" }
    ]
  }